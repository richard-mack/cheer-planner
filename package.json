    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "protractor": "^4.0.14",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "typescript": "^2.1.6",
    "zone.js": "^0.8.4"
  },
  "author": ""
}
